{
  "useGitignore": true,
  "numSuggestions": 20,
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    "node_modules"
  ],
  "dictionaries": [
    "custom-words"
  ],
  "dictionaryDefinitions": [
    {
      "name": "custom-words",
      "path": "~/.cspell/custom-words.txt",
      "addWords": true
    }
  ],
  "words": [
    "blankline",
    "dockerls",
    "numhl",
    "noremap",
    "setloclist",
    "bufnr",
    "omnifunc",
    "bufopts",
    "sumneko",
    "pyright",
    "denols",
    "bashls",
    "cssls",
    "angularls",
    "prismals",
    "texthl",
    "inlayhints",
    "vimgrep",
    "nvim",
    "neodev",
    "lspconfig",
    "floaterm",
    "bprevious",
    "bnext",
    "nohlsearch",
    "cursorline",
    "noshowmode",
    "noshowcmd",
    "noselect",
    "shiftwidth",
    "expandtab",
    "smartindent",
    "mapleader",
    "augroup",
    "autocmd",
    "foldlevel",
    "foldlevelstart",
    "foldenable",
    "foldclose",
    "nodigits",
    "tresitter",
    "unnamedplus",
    "ignorecase",
    "breakindent"
  ]
}
